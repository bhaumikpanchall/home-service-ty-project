<%- include('header'); -%>
    <div id="wrapper" class="inner_page home_two contacts_page">

        <section id="Contact_form">

            <div class="container my-4">
                <h1>My Orders</h1>
                <table class="table">

                    <tr>
                        <th scope="col">Username</th>
                        <th scope="col">Category Name</th>
                        <th scope="col">Booking Date</th>
                        <th scope="col">Total Amount</th>
                        <th scope="col">Booking status</th>
                        <th scope="col">Payment Status</th>
                        <th scope="col">Payment Type</th>
                        <th scope="col">View Details</th>

                    </tr>

                    <tbody>

                        <% data.forEach((item)=>{ %>
                            <tr>
                                <td>
                                    <%= item.User.Fname + " " + item.User.Lname %>
                                </td>
                                <td>
                                    <%= item.Category_Booking.category%>
                                </td>
                                <td>
                                    <%= item.Booking_date%>
                                </td>
                                <td>
                                    <%= item.Total_Amount%>
                                </td>
                                <td>
                                    <%= item.Booking_status %>
                                </td>
                                <td>
                                    <%= item.Payment_status %>
                                </td>
                                <td>
                                    <%= item.Payment_Type %>
                                </td>
                                <td>
                                    <a href="/serviceprovider/order/<%=item.id%>"> View Details</a>
                                </td>
                            </tr>
                            <% }) %>
                    </tbody>
                </table>
            </div>

        </section>
        <!--Contact Form End-->

        <!--Footer-->
        <%- include('footer'); -%>
            <!--/Footer-->

    </div>
    <!--/Wrapper End-->

